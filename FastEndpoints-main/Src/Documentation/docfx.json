{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "api-filter.yml",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "wiki/**.md",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "CNAME"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../../docs",
    "globalMetadata": {
      "_appTitle": "FastEndpoints",
      "_description": "A light-weight REST Api framework for ASP.Net 6 that implements REPR (Request-Endpoint-Response) Pattern",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_appLogoPath": "images/icon.png",
      "_appFaviconPath": "images/favicon.ico",
      "_appFooter": "Developed by <a href='https://github.com/dj-nitehawk'>Đĵ ΝιΓΞΗΛψΚ</a> and <a href='https://github.com/dj-nitehawk/FastEndpoints/graphs/contributors'>contributors</a> / Licensed under <a href='https://github.com/dj-nitehawk/FastEndpoints/blob/master/LICENSE'>MIT</a> / Website generated by <a href='https://dotnet.github.io/docfx/'>DocFX</a>"
    },
    "template": [
      "default",
      "templates\\custom"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}